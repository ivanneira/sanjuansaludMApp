<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="es-ES" id="io.cordova.myappf54e80" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>SanJuanSaludAPP</name>
    <description>Un proyecto en blanco que usa Apache Cordova para ayudarle a compilar una aplicación que tiene varias plataformas móviles como destino: Android, iOS y la plataforma universal de Windows.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Su nombre aquí</author>
    <vs:template-name>BlankJS</vs:template-name>
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="loadUrlTimeoutValue" value="60000" />
    <preference name="fullscreen" value="false" />
    <hook src="scripts/appBeforeBuild.js" type="before_build" />
    <platform name="android">
        <preference name="android-manifest/application/activity/@android:windowSoftInputMode" value="adjustResize" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="res/icons/android/logo.png" />
